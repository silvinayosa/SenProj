<style>
  
  .card {
  width: 228px;
  height: 305px;
  background: #f0f0f0;
  border-radius: 10px;
  text-align: center;
  transition: all 0.5s;
  position: relative;
  }

  .card:hover {
  box-shadow: 0 0 40px 2px rgba(0, 0, 0, 0.623);
  background-color: #4bb8ff;
  }

  .card .blob {
  height: 15px;
  width: 85%;
  border-radius: 0 0 30px 30px;
  margin: 0 auto;
  background-color: #4bb8ff;
  visibility: visible;
  transition: all 0.3s;
  }

  .card:hover .blob {
  height: 0;
  }

  .card .img {
  display: flex;
  margin: 38px auto 10px auto;
  width: 77px;
  height: 77px;
  background-color: #4bb8ff;
  border-radius: 50%;
  font-size: 11px;
  justify-content: center;
  align-items: center;
  transition: all 0.5s;
  }

  .card:hover .img {
  width: 100%;
  height: 77%;
  border-radius: 10px 0 0;
  margin: 0 auto;
  background-color: #f0f0f0;
  z-index: 99999;
  }

  .card h2 {
  padding: 17px 10px;
  font-size: 31px;
  transition: all 0.1s;
  z-index: -99;
  line-height: 17px;
  }

  .card span {
  font-size: 21px;
  }

  .card:hover h2 {
  opacity: 0;
  width: 100%;
  position: absolute;
  transition: all 0.5s;
  }

  .card>p {
  opacity: 0;
  transition: all 0.75s;
  }

  .card>p>svg {
  padding: 6px;
  display: inline-block;
  }

  .card:hover>p {
  position: absolute;
  bottom: 19px;
  left: 41px;
  opacity: 1;
  transition: all 0.1s;
  }

  .card .img .profile-image {
  width: 100%; /* Ensure the image takes up the full width of the container */
  height: 100%; /* Ensure the image takes up the full height of the container */
  object-fit: cover; /* This ensures the image fills the div without distortion */
  border-radius: 50%; /* Make the image circular */
  }
  .card:hover .img .profile-image {
  border-radius: 10px 0 0; /* Change the border radius on hover */
  }
  
  /* Add this to make the specific card orange */
  .orange-card:hover {
  background-color: #ff8c00 !important; /* A slightly darker orange on hover */
  }

  .orange-card .blob {
  background-color: #ff8c00 !important; /* Orange blob */
  }

  .orange-card .img {
  background-color: #ff8c00 !important; /* Orange circle */
  }

  .orange-card:hover .img {
  background-color: #f0f0f0 !important; /* Revert the circle color on hover */
  }
</style>

